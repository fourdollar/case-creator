/* pages/form/index.wxss */
.page-body {
  padding: 10px 0;
}
.page-section {
  margin-bottom: 10px;
}
.weui-cells__title {
  margin-top: .77em;
  margin-bottom: .3em;
  padding-left: 15px;
  padding-right: 15px;
  color: #999999;
  font-size: 32rpx;
}
.weui-cells_after-title {
  margin-top: 0;
}
.weui-cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #D9D9D9;
  color: #D9D9D9;
}
.weui-cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #D9D9D9;
  color: #D9D9D9;
}
.weui-cells {
  position: relative;
  margin-top: 15rpx;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 30rpx;
}
.weui-cell_input {
  padding-top: 0;
  padding-bottom: 0;
}
.weui-cell {
  padding: 10px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.submit-button {
  position: absolute;
  width: 100%;
  bottom: 30rpx;
}